	// Get the latest resource available in the k8s cluster
	// This in can be done in a similar way



    			if uObj.GetNamespace() != "kube-system" {
				c.queue.Add(k8sResource{
					Name:        uObj.GetName(),
					Namespace:   uObj.GetNamespace(),
					ResourceTye: strings.ToLower(uObj.GroupVersionKind().Kind) + "s",
				})
			}



            // Custom implementation of Queue Elements
// to be used for all operations in WorkQueue
type k8sResource struct {
	Name        string `json:"name"`
	Namespace   string `json:"namespace"`
	ResourceTye string `json:"resource"`
}